{
  "private": true,
  "name": "cameras",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "preact build --no-prerender",
    "serve": "sirv build --port 8080 --cors --single --http2 --cert ./fullchain.pem --key ./privkey.pem --host central-2.us.paradigmpost.com",
    "dev": "preact watch --https --cert ./fullchain.pem --key ./privkey.pem",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "jest ./tests",
    "rtcsession:dev": "npm run build && npm run serve"
  },
  "lint-staged": {
    "*.{css,md,scss}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "preact": "^10.3.1",
    "preact-jsx-chai": "^3.0.0",
    "preact-markup": "^2.0.0",
    "preact-render-to-string": "^5.1.4"
  },
  "devDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.2.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.8",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^26.2.2",
    "jest-preset-preact": "^4.0.2",
    "preact-cli": "^3.0.0",
    "prettier": "^1.19.1",
    "sirv-cli": "^1.0.0-next.3",
    "typescript": "^3.7.5"
  }
}
